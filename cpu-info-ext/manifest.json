{
    "manifest_version": 3,
    "name": "Webbench CPU Info Helper",
    "version": "4.2.0",
    "permissions": [
        "system.cpu",
        "system.memory"
    ],
    "background": {
        "service_worker": "src/sw.js"
    },
    "content_scripts": [{
        "matches": [
            "https://mayfield.github.io/webbench/*",
            "http://jm:8000/*"
        ],
        "run_at": "document_start",
        "js": ["src/getcpuinfo.js"]
    }]
}
